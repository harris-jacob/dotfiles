Xft.dpi: 102
!Xft.dpi: 96
Xft.antialias: true
Xft.hinting: true
Xft.rgba: rgb
Xft.autohint: false
Xft.hintstyle: hintfull
Xft.lcdfilter: lcddefault

xcursor.theme:            Vanilla-DMZ
xscreensaver.fade:   false
xscreensaver.unfade: false
! ===== urxvt =====
URxvt.scrollBar: off
URxvt.font: xft:monospace:size=12
URxvt.internalBorder:     22
! =================
*background:    #1d212s
*foreground:    #1d212a
*cursorColor:   #e3e3e3


! black
*color0:       #1d212a
*color8:       #585e74

! red
*color1:       #fc8993
*color9:       #fb9199

! green
*color2:       #89e19c
*color10:      #8de19f

! yellow
*color3:       #fbdf90
*color11:      #ffe28e

! blue
*color4:       #a7bdfb
*color12:      #afc3fb

! magenta
*color5:       #d990cd
*color13:      #df95d3

! cyan
